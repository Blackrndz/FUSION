        <block name="CHECK_TEMPLATE">
            <IfThenElse>
                <if>
                    <comparisons separator="AND">
                        <comparison operator="EQUAL">
                            <leftOperand>
                                <valueKB inventoryName="MAIN_REPEAT_1" columnName="Document Type"/>
                            </leftOperand>
                            <rightOperand>
                                <valueText/>
                            </rightOperand>
                        </comparison>
                    </comparisons>
                </if>
                <then>
                    <error message="Please specify column 'Document Type' for Create Templates."/>
                </then>
                <else/>
            </IfThenElse>
