		<block name="Item_Management">
			<IfThenElse>
				<if>
					<findElement by="xpath">//a[contains(@class,'Select')][text()='Item Management']</findElement>
				</if>
				<then/>
				<else>
					<findElement by="xpath">//a[text()='Item Management']</findElement>
					<click waitForPageToFinishRendering="true"/>
				</else>
			</IfThenElse>
			<IfThenElse>
				<if>
					<findElement isDisplayed="true" setFocus="true" by="xpath">//a[@title='Expand Number Generation']</findElement>
				</if>
				<then>
					<findElement isDisplayed="true" setFocus="true" by="xpath">//a[@title='Expand Number Generation']</findElement>
					<click waitForPageToFinishRendering="true"/>
					<pause durationInSecs="1"/>
				</then>
				<else/>
			</IfThenElse>
			<!--Item Number Generation Method-->
			<executeBlock navigationName="TemplateSELECT" name="TEMPATE_SELECT">
				<parameters>
					<parameter name="XPATH" value="//label[text()='Item Number Generation Method']/../.."/>
					<parameter name="INV_NAME" value="MAIN_4_REPEAT"/>
					<parameter name="COL_NAME" value="Item Number Generation Method"/>
				</parameters>
			</executeBlock>	
			
			<IfThenElse>
				<if>
					<comparisons separator="AND">
						<comparison operator="NOT_EQUAL">
							<leftOperand>
								<valueKB inventoryName="MAIN_4_REPEAT" columnName="Item Number Generation Method - Associated Rule Set"/>
							</leftOperand>
							<rightOperand>
								<valueText/>
							</rightOperand>
						</comparison>
					</comparisons>
				</if>
				<then>
					<IfThenElse>
						<if>
							<findElement isDisplayed="true" by="xpath">//label[text()='Item Number Generation Method']/../..//*[@title='Rule Generated']</findElement>
						</if>
						<then>
							<executeBlock navigationName="TemplateSELECT" name="TEMPATE_SELECT">
								<parameters>
									<parameter name="XPATH" value="(//label[text()='Associated Rule Set']/../..//select)[1]/.."/>
									<parameter name="INV_NAME" value="MAIN_4_REPEAT"/>
									<parameter name="COL_NAME" value="Item Number Generation Method - Associated Rule Set"/>
								</parameters>
							</executeBlock>	
						</then>
						<else/>
					</IfThenElse>
				</then>
				<else/>
			</IfThenElse>
			<!--Starting Number-->
			<IfThenElse>
				<if>
					<comparisons separator="AND">
						<comparison operator="NOT_EQUAL">
							<leftOperand>
								<valueKB inventoryName="MAIN_4_REPEAT" columnName="Item Number Generation Method - Starting Number"/>
							</leftOperand>
							<rightOperand>
								<valueText/>
							</rightOperand>
						</comparison>
					</comparisons>
				</if>
				<then>
					<IfThenElse>
						<if>
							<findElement isDisplayed="true" by="xpath">//label[text()='Item Number Generation Method']/../..//*[@title='Sequence Generated']</findElement>
						</if>
						<then>
							<executeBlock navigationName="TemplateINPUT" name="TEMPLATE_INPUT">
								<parameters>
									<parameter name="XPATH" value="(//label[text()='Starting Number']/../..//input)[1]/.."/>
									<parameter name="INV_NAME" value="MAIN_4_REPEAT"/>
									<parameter name="COL_NAME" value="Item Number Generation Method - Starting Number"/>
								</parameters>
							</executeBlock>
