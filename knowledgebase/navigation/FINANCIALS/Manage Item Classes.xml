					<parameter
                            name="SCROLLBAR_GENERIC_2_FIND_ROW">//table[@summary='Attachment Categories']//td[1][text()='<valueKB
                            inventoryName="MAIN_3_REPEAT" columnName="Attachment Category"/>']</parameter>
					<parameter
                            name="SCROLLBAR_GENERIC_3_FIND_ROW">//table[@summary='Attachment Categories']//tr[td[2][text()='NEED TO CREATE']]</parameter>
					<parameter
                            name="SCROLLBAR_GENERIC_4_FIND_ROW">//table[@summary='Attachment Categories']//tr[td[2][text()='NEED TO CREATE']]</parameter>
