		</block>
		
		<block name="Address_EDIT">
			<IfThenElse>
				<if>
					<findElement by="xpath">//table[@summary='Addresses']/../../../..//div[@title='Edit']/table//td/a</findElement>
				</if>
				<then>
					<findElement by="xpath">//table[@summary='Addresses']/../../../..//div[@title='Edit']/table//td/a</findElement>
					<click waitForPageToFinishRendering="true"/>
					<findElement by="xpath">//table[@style='position: relative;']//*[text()='Edit address']</findElement>
					<click waitForPageToFinishRendering="true"/>
					<pause durationInSecs="2"/>
				</then>
				<else>
					<findElement by="xpath">//table[@summary='Addresses']/../../../..//*[text()='Actions']</findElement>
					<click waitForPageToFinishRendering="true"/>
					<findElement by="xpath">//table[@style='position: relative;']//*[text()='Edit address']</findElement>
					<click waitForPageToFinishRendering="true"/>
					<pause durationInSecs="2"/>
				</else>
			</IfThenElse>
		</block>
		<block name="Address_OK">
			<findElement by="xpath">//div[contains(text(),'Address')]/../../../../../../..//*[text()='K']/..</findElement>
			<click waitForPageToFinishRendering="true"/>
			<pause durationInSecs="2"/>
		</block>
		<block name="Search_address">
			<defineVariable name="VALUE_ADDRESS" type="text"/>
			<setVariable name="VALUE_ADDRESS" append="false">
				<valueText/>
			</setVariable>
			<IfThenElse>
				<if>
					<comparisons separator="AND">
						<comparison operator="NOT_EQUAL">
							<leftOperand>
								<valueKB inventoryName="REPEAT_1_MAIN" columnName="Site Number"/>
							</leftOperand>
							<rightOperand>
								<valueText/>
							</rightOperand>
						</comparison>
					</comparisons>
				</if>
				<then>
					<setVariable name="VALUE_ADDRESS" append="true">//table[@summary='Addresses']//td[text()='</setVariable>
					<setVariable name="VALUE_ADDRESS" append="true">
						<valueKB inventoryName="REPEAT_1_MAIN" columnName="Site Number"/>
					</setVariable>
					<setVariable name="VALUE_ADDRESS" append="true">']</setVariable>
				</then>
				<else>
					<setVariable name="VALUE_ADDRESS" append="true">//table[@summary='Addresses']//tr//td//*[@title='Primary']/../../td[1]</setVariable>
				</else>
			</IfThenElse>		
			
			
			<executeBlock navigationName="TemplateSCROLLBAR" name="TEMPLATE_SCROLLBAR">
                <parameters>
                    <parameter name="TEMPLATE_NO_DATA" value="//table[@summary='Addresses']/../..//div[contains(text(),'No data to display.')]"/>
                    <parameter name="TEMPLATE_FIRST_ROW" value="(//table[@summary='Addresses'][@_rowcount]//tr[1]//td)[1]"/>
                    <parameter name="TEMPLATE_SCROLLER" value="//table[@summary='Addresses']/../..//div[contains(@id,'scroller')]"/>
                    <parameter name="TEMPLATE_1_FIND_ROW"><valueVariable name="VALUE_ADDRESS"/></parameter>
                    <parameter name="TEMPLATE_CREATE" value="CREATE_ADDRESS"/>
                    <parameter name="TEMPLATE_UPDATE" value="UPDATE_ADDRESS"/>
                </parameters>
            </executeBlock>
		</block>		
		<executeBlock navigationName="TemplateTASK" name="TEMPLATE_GO_TO_TASK"/>
		<repeat inventoryName="REPEAT_1_MAIN">
			<executeBlock name="Search_LA"/>
			<pause durationInSecs="2"/>

			<executeBlock name="Search_address"/>
			<repeat inventoryName="REPEAT_2_MAIN">
				<executeBlock navigationName="TemplateSCROLLBAR" name="TEMPLATE_SCROLLBAR">
					<parameters>
						<parameter name="TEMPLATE_NO_DATA" value="//table[@summary='Address Purposes']/../..//div[contains(text(),'No data to display.')]"/>
						<parameter name="TEMPLATE_FIRST_ROW" value="(//table[@summary='Address Purposes'][@_rowcount]//tr[1]//td)[1]"/>
						<parameter name="TEMPLATE_SCROLLER" value="//table[@summary='Address Purposes']/../..//div[contains(@id,'scroller')]"/>
						<parameter name="TEMPLATE_1_FIND_ROW">//table[@summary='Address Purposes']//select[@title='<valueKB inventoryName="REPEAT_2_MAIN" columnName="Purpose"/>']</parameter>
						<parameter name="TEMPLATE_CREATE" value="CREATE_Address_Purposes"/>
						<parameter name="TEMPLATE_UPDATE" value="UPDATE_Address_Purposes"/>
					</parameters>
				</executeBlock>				
			</repeat>
			<executeBlock name="Address_OK"/>
			<executeBlock navigationName="TemplateUTILITY" name="TEMPLATE_SAVE"/>
			<pause durationInSecs="2"/>
			<executeBlock navigationName="TemplateUTILITY" name="TEMPLATE_CONFIRMATION"/>
			<pause durationInSecs="2"/>
			<executeBlock navigationName="TemplateUTILITY" name="TEMPLATE_SAVE_AND_CLOSE"/>
		</repeat>
