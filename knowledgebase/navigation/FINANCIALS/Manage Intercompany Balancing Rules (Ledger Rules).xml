				<if>
					<comparisons separator="AND">
						<comparison operator="NOT_EQUAL">
							<leftOperand>
								<valueKB inventoryName="REPEAT_1_MAIN" columnName="Source"/>
							</leftOperand>
							<rightOperand>
								<valueText/>
