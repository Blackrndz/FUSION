		<block name="Construction_of_Rule_Logic">
			<IfThenElse>
				<if>
					<comparisons separator="AND">
						<comparison operator="NOT_EQUAL">
							<leftOperand>
								<valueKB inventoryName="INV_2" columnName="Selected"/>
							</leftOperand>
							<rightOperand>
								<valueText/>
							</rightOperand>
						</comparison>
					</comparisons>
				</if>
				<then>
					<IfThenElse>
						<if>
							<findElement by="xpath">//td/*[text()='Available']/../../../../..//label[text()='<valueKB inventoryName="INV_2" columnName="Selected"/>']/..//input</findElement>
						</if>
						<then>
							<findElement by="xpath">//td/*[text()='Available']/../../../../..//label[text()='<valueKB inventoryName="INV_2" columnName="Selected"/>']/..//input</findElement>
							<click waitForPageToFinishRendering="true"/>
							<findElement by="xpath">(//table[contains(@id,'lifeEventShuttle::content')]//a)[1]</findElement>
							<click waitForPageToFinishRendering="true"/>
						</then>
						<else/>
					</IfThenElse>
				</then>
				<else/>
			</IfThenElse>
		</block>
		
