        <block name="SCROLLBAR_PARENT">
            <!-- check if there is no data found returned after the search, if so then we create a new record if applicable -->
            <IfThenElse>
                <if>
                    <findElement isSkipCheckDomError="true" by="xpath">SCROLLBAR_GENERIC_1_NO_DATA</findElement>
                </if>
                <then>
                    <executeBlock name="SCROLLBAR_GENERIC_1_CREATE_BLOCK"/>
                </then>
                <else>
                    <IfThenElse>
                        <if>
                            <findElement isDisplayed="true" by="xpath" isSkipCheckDomError="true">SCROLLBAR_GENERIC_1_SCROLLER</findElement>
                        </if>
                        <then>
                            <!-- resetting the scrollbar back to the Top because the form remembers where it was even when searching a new record -->
                            <findElement highlightTurnedOff="true"
                                isSkipCheckDomError="true" by="xpath">SCROLLBAR_GENERIC_1_SCROLLER</findElement>
                            <scroll direction="TOP"/>
                            <findElement highlightTurnedOff="true"
                                by="xpath" isSkipCheckDomError="true" setFocus="true">SCROLLBAR_GENERIC_1_FIRST_ROW</findElement>
                            <click waitForPageToFinishRendering="true" isSkipCheckDomError="true"/>
                            <findElement by="xpath">//button[@title='Clear']</findElement>
                            <click waitForPageToFinishRendering="true"/>
                            <executeBlock name="SCROLLBAR_INNER_PARENT" isRepeatUntilBreak="true"/>
                        </then>
                        <else>
                            <executeBlock name="NO_SCROLLBAR_INNER_PARENT"/>
                        </else>
                    </IfThenElse>
                </else>
            </IfThenElse>
        </block>
        <block name="SCROLLBAR_INNER_PARENT">
            <IfThenElse>
