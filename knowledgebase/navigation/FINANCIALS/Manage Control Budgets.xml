		<block name="Status">
			<IfThenElse>
				<if>
					<comparisons separator="AND">
						<comparison operator="EQUAL">
							<leftOperand>
								<valueKB inventoryName="INV_1" columnName="Status"/>
							</leftOperand>
							<rightOperand>
								<valueText>In use</valueText>
							</rightOperand>
						</comparison>
					</comparisons>
				</if>
				<then>
					<IfThenElse>
						<if>
							<findElement by="xpath">//label[text()='Status']/../..//*[text()='In use']</findElement>
						</if>
						<then/>
